{
  "name": "lets-try-keyboard-layouts",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --local",
    "dev": "run-p \"dev:*\"",
    "type-check": "tsc --noEmit",
    "deploy": "cross-env NODE_ENV=production wrangler pages publish ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.9.0",
    "@remix-run/cloudflare-pages": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@mui/base": "5.0.0-alpha.112",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "daisyui": "^2.46.0",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "recoil": "^0.7.6",
    "recoil-persist": "^4.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  },
  "volta": {
    "node": "16.13.0"
  }
}
