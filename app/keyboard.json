{
  "qwerty": {
    "name": "QWERTY",
    "layout": [
      [
        ["1", "!"],
        ["2", "@"],
        ["3", "#"],
        ["4", "$"],
        ["5", "%"],
        ["6", "^"],
        ["7", "&"],
        ["8", "*"],
        ["9", "("],
        ["0", ")"],
        ["-", "_"],
        ["=", "+"]
      ],
      [
        "q",
        "w",
        "e",
        "r",
        "t",
        "y",
        "u",
        "i",
        "o",
        "p",
        ["[", "{"],
        ["]", "}"]
      ],
      ["a", "s", "d", "f", "g", "h", "j", "k", "l", [";", ":"], ["'", "\""]],
      ["z", "x", "c", "v", "b", "n", "m", [",", "<"], [".", ">"], ["/", "?"]]
    ]
  },
  "dvorak": {
    "name": "Dvorak",
    "layout": [
      [
        ["1", "!"],
        ["2", "@"],
        ["3", "#"],
        ["4", "$"],
        ["5", "%"],
        ["6", "^"],
        ["7", "&"],
        ["8", "*"],
        ["9", "("],
        ["0", ")"],
        ["[", "{"],
        ["]", "}"]
      ],
      [
        "'",
        ",",
        ".",
        "p",
        "y",
        "f",
        "g",
        "c",
        "r",
        "l",
        ["/", "?"],
        ["=", "+"]
      ],
      ["a", "o", "e", "u", "i", "d", "h", "t", "n", "s", ["-", "_"]],
      [";", "q", "j", "k", "x", "b", "m", "w", "v", "z"]
    ]
  },
  "colemak": {
    "name": "Colemak",
    "layout": [
      [
        ["1", "!"],
        ["2", "@"],
        ["3", "#"],
        ["4", "$"],
        ["5", "%"],
        ["6", "^"],
        ["7", "&"],
        ["8", "*"],
        ["9", "("],
        ["0", ")"],
        ["-", "_"],
        ["=", "+"]
      ],
      [
        "q",
        "w",
        "f",
        "p",
        "g",
        "j",
        "l",
        "u",
        "y",
        [";", ":"],
        ["[", "{"],
        ["]", "}"]
      ],
      ["a", "r", "s", "t", "d", "h", "n", "e", "i", ["o", "O"], ["'", "\""]],
      ["z", "x", "c", "v", "b", "k", "m", [",", "<"], [".", ">"], ["/", "?"]]
    ]
  }
}
